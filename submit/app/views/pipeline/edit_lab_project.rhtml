<br/>
<b>Change lab for submission '<%= @project.name -%>':</b>

<% form_tag :action => 'edit_lab_project', :id => @project do %>
  <label for="project_lab">Lab</label>
  <%= select_tag 'project_pi_and_lab', grouped_options_for_select(@acceptable_labs, "#{@project.pi}/#{@project.lab}") %><br/>
  <div style="float: left; margin-right: 15px;">
    <%= submit_tag 'Save' -%>
  </div>
<% end %>
<div style="float: left"><%= button_to 'Cancel', :action => :show, :id => @project.id %></div>

